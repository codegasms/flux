            </svg>
            <svg class="swap-on fill-current w-[24px] h-[24px] contrast-0" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
              <path d="M21.64,13a1,1,0,0,0-1.05-.14,8.05,8.05,0,0,1-3.37.73A8.15,8.15,0,0,1,9.08,5.49a8.59,8.59,0,0,1,.25-2A1,1,0,0,0,8,2.36,10.14,10.14,0,1,0,22,14.05,1,1,0,0,0,21.64,13Zm-9.5,6.69A8.14,8.14,0,0,1,7.08,5.22v.27A10.15,10.15,0,0,0,17.22,15.63a9.79,9.79,0,0,0,2.1-.22A8.11,8.11,0,0,1,12.14,19.73Z" />
            </svg>
          </label>
          <button class="btn btn-sm btn-ghost px-[6px]">
            <img src="/images/space/notifications.png" class="contrast-0" />
          </button>
          <div>
            <div class="divider divider-horizontal mx-[2px] h-[24px]"></div>
          </div>
          <button class="btn btn-sm btn-ghost px-[6px]">
            <img src="/images/space/user.png" class="contrast-0" />
            Team Codegasms
          </button>
          <button class="btn btn-sm btn-ghost px-[6px]">
            <img src="/images/space/logout.png" class="contrast-0" />
          </button>
        </div>
      </div>
    </div>

    <!-- Main page -->
    <div class="px-8 w-full overflow-y-scroll no-scrollbar">
      <!-- My Storage Header -->
      <div class="flex justify-between w-full my-4">
        <h1 class="text-[24px] font-['Quicksand'] font-bold">Users</h1>
        <button class="btn btn-outline" id="createPlan">Create Plan</button>
        <script>
          const createPlan = document.querySelector('#createPlan');
          createPlan.addEventListener('click', () => {
            window.location.href = '/admin/tokenplans';
          });
        </script>
      </div>
      <!-- Analytics -->
      <% const users = [
      {
        index: 0,
        name: 'Free',
        country: 'Price: $0',
        company: 'Limited features with basic file conversion and storage capabilities.',
